  "license": "BSD-3-Clause",
  "sideEffects": false,
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/types/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    "import": "./dist/index.mjs",
    "require": "./dist/index.cjs",
