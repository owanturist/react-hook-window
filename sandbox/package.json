{
  "private": true,
  "name": "sandbox",
  "version": "1.0.0",
  "scripts": {
    "clean": "rm -rf dist",
    "typecheck": "tsc -p ./tsconfig.json --noEmit",
    "dev": "parcel ./public/index.html",
    "build": "parcel build ./public/index.html --out-file=200.html --no-cache",
    "prebuild": "yarn run clean"
  },
  "dependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.2.0",
    "react-hook-window": "*"
  },
  "devDependencies": {
    "@types/react": "^17.0.11",
    "@types/react-dom": "^17.0.8",
    "@types/react-router-dom": "^5.1.7",
    "parcel-bundler": "^1.12.5"
  }
}
